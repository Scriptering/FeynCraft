; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="FeynCraft"
run/main_scene="res://Scenes and Scripts/universe.tscn"
config/features=PackedStringArray("4.2")
run/low_processor_mode=true
boot_splash/bg_color=Color(0.254902, 0.25098, 0.258824, 1)
boot_splash/image="res://Textures/splash_icon.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Textures/game_icon.png"
config/macos_native_icon="res://Textures/game_icon.icns"
config/windows_native_icon="res://Textures/game_icon.ico"

[autoload]

GLOBALS="*res://Scenes and Scripts/GLOBALS.gd"
SOUNDBUS="*res://Scenes and Scripts/sound_bus.tscn"
EVENTBUS="*res://Scenes and Scripts/event_bus.gd"
ParticleData="*res://ParticleData.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=3
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0
mouse_cursor/custom_image_hotspot=Vector2(30, 7)
mouse_cursor/tooltip_position_offset=Vector2(-5, 10)

[filesystem]

import/blender/enabled=false

[global]

export=false

[gui]

theme/custom="res://Resources/Themes/ProjectTheme.tres"
common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
